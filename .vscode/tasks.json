{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "flutter-run-once",
			"type": "shell",
			"command": "flutter",
			"args": [
				"run"
			],
			"problemMatcher": [
				"$dart"
			],
			"group": "build"
		},
		{
			"label": "flutter-get-deps",
			"type": "shell",
			"command": "flutter",
			"args": [
				"pub",
				"get"
			],
			"problemMatcher": [
				"$dart-json"
			],
			"group": "build"
		},
		{
			"label": "flutter-run-once",
			"type": "shell",
			"command": "flutter",
			"args": [
				"run"
			],
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		},
		{
			"label": "flutter-run-once",
			"type": "shell",
			"command": "flutter",
			"args": [
				"run",
				"--android-skip-build-dependency-validation"
			],
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}